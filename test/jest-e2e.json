{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["<rootDir>/../src/**/*.(t|j)s?(x)"],
  "coveragePathIgnorePatterns": [
    ".module.ts",
    "spec.ts",
    "<rootDir>/../src/main.ts"
  ]
}
